class Solution {
public:
    int countPrefixes(vector<string>& words, string s) {
        int ans = 0;

        for (int i = 0; i < words.size(); i++)
        {
            bool check = true;
            for (int j = 0; j < words[i].length(); j++)
            {
                if (s[j] != words[i][j])
                {
                    check = false;
                    break;
                }
            }
            if (check)
                ans++;
        }

        return ans;
    }
};
