class Solution {
public:
    bool checkString(string s) {
        bool flag = false;
        
        for (int i = s.length()-1; i >= 0; i--)
        {
            if (s[i] == 'a')
                flag = true;
            if (s[i] == 'b' && flag == true)
                return false;
        }
        return true;
    }
};
